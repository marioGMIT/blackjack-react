{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        // "eslint:recommended",
        // "plugin:react/recommended",
        "airbnb",
        "prettier"
        // "plugin:node/recommended",

    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars":"off",
        "no-console":"error",
        "func-name":"off"
    }
}
